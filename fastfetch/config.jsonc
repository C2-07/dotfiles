{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "type": "kitty", // or "sixel" or "iterm2" depending on your terminal
    "source": "/Users/gourav/.config/fastfetch/shell_logo.png",
    "width": 35,
    "height": 18,
    "padding": {
      "top": 3,
      "right": 4,
      "left": 0,
    },
    "preserveAspectRatio": true,
  },
  "display": { "separator": "  " },
  "modules": [
